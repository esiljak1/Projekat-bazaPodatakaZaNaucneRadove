<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane fx:id="gridMain" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/MainWindowStyle.css" />
    </stylesheets>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="269.0" minWidth="10.0" prefWidth="269.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="61.6" minHeight="10.0" prefHeight="45.6" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="61.6" minHeight="10.0" prefHeight="45.6" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="61.6" minHeight="10.0" prefHeight="45.6" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Button fx:id="loginBtn" mnemonicParsing="false" onAction="#loginAction" text="LOGIN" GridPane.rowIndex="4" GridPane.vgrow="ALWAYS">
         <GridPane.margin>
            <Insets bottom="2.0" left="10.0" right="2.0" top="2.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="guestBtn" mnemonicParsing="false" text="CONTINUE AS GUEST" GridPane.halignment="RIGHT" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </GridPane.margin>
      </Button>
      <Label text="Or" GridPane.halignment="CENTER" GridPane.rowIndex="4" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets right="70.0" />
         </GridPane.margin>
      </Label>
      <ImageView fx:id="imgLogo" fitHeight="160.0" fitWidth="250.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets bottom="15.0" left="5.0" right="5.0" top="5.0" />
         </GridPane.margin></ImageView>
      <Label fx:id="welcomeMsg" text="Welcome to SWW" GridPane.halignment="CENTER" GridPane.rowIndex="3" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets top="28.0" />
         </GridPane.margin></Label>
   </children>
</GridPane>
