<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@/css/MainWindowStyle.css" />
    </stylesheets>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="129.0" minHeight="10.0" prefHeight="75.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="244.0" minHeight="10.0" prefHeight="244.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <TextArea fx:id="tagsTxt" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets left="10.0" right="10.0" />
         </GridPane.margin></TextArea>
      <Label fx:id="tagsLbl" text="Please enter tags (separate with comma)" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <ButtonBar prefHeight="40.0" prefWidth="200.0" GridPane.rowIndex="2">
        <buttons>
          <Button fx:id="okBtn" mnemonicParsing="false" text="OK" onAction="#closeAction" />
            <Button fx:id="cancelBtn" minWidth="54.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Cancel" onAction="#closeAction" />
        </buttons>
         <GridPane.margin>
            <Insets right="70.0" />
         </GridPane.margin>
      </ButtonBar>
   </children>
</GridPane>
